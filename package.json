    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/app/test/jest-e2e.json",
    "db:create": "node ./create-db-script.js",
    "db:drop": "node ./drop-db-script.js",
    "migration:run": "npx typeorm migration:run",
    "migration:create": "npx typeorm migration:create -d ./src/db/migrations -n ",
    "migration:generate": "npx typeorm migration:generate -d ./src/db/migrations -n ",
    "migration:revert": "npx typeorm migration:revert",
    "console": "node dist/console.js"
